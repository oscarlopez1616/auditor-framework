{
    "type": "project",
    "license": "proprietary",
    "_comment": "# review this requirement when trikoder will update version to the next relase",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mysql": "*",
        "ext-openssl": "*",
        "ext-soap": "*",
        "ext-sodium": "*",
        "devio/pipedrive": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.6",
        "guzzlehttp/guzzle": "~6.3",
        "jms/serializer-bundle": "^3.3",
        "kamermans/guzzle-oauth2-subscriber": "^1.0",
        "lambdish/phunctional": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/iso3166": "^2.1",
        "mandrill/mandrill": "^1.0",
        "moneyphp/money": "^3.2",
        "nelmio/cors-bundle": "^1.5",
        "nyholm/psr7": "^1.1",
        "oneup/flysystem-bundle": "^3.0",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "prooph/event-sourcing": "^5.6",
        "prooph/event-store-bus-bridge-symfony-bundle": "dev-master",
        "prooph/event-store-symfony-bundle": "^0.6.0",
        "prooph/pdo-event-store": "^1.11",
        "prooph/service-bus-symfony-bundle": "^0.7.0",
        "prooph/snapshot-store": "^1.3",
        "react/event-loop": "^1.1",
        "react/promise": "^2.7",
        "reactivex/rxphp": "^2.0",
        "rx/websocket": "^2.1",
        "scienta/doctrine-json-functions": "^4.0",
        "sensio/framework-extra-bundle": "^5.3",
        "signaturit/signaturit-sdk": "^1.1",
        "symfony/console": "^4.3",
        "symfony/flex": "^1.2",
        "symfony/framework-bundle": "^4.3",
        "symfony/intl": "~4.3",
        "symfony/monolog-bundle": "^3.3",
        "symfony/process": "~4.3",
        "symfony/proxy-manager-bridge": "^4.3",
        "symfony/security-bundle": "^4.3",
        "symfony/serializer": "^4.3",
        "symfony/validator": "~4.3",
        "symfony/web-profiler-bundle": "^4.3",
        "symfony/web-server-bundle": "^4.3",
        "symfony/yaml": "^4.3",
        "trikoder/oauth2-bundle": "dev-master",
        "zendesk/zendesk_api_client_php": "^2.2",
        "zircote/swagger-php": "^3.0"
    },
    "require-dev": {
        "symfony/dotenv": "^4.3",
        "symfony/phpunit-bridge": "^4.3",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AuditorFramework\\": "src",
            "App\\": "App"
        }
    },
    "autoload-dev": {
        "psr-4": {

        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "rm -rf tests",
            "rm -rf src/Controller",
            "rm -rf src/Entity",
            "rm -rf src/Repository",
            "rm -rf src/Consumer",
            "rm -rf src/Command",
            "rm -rf src/Migrations"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "rm -rf tests",
            "rm -rf src/Controller",
            "rm -rf src/Entity",
            "rm -rf src/Repository",
            "rm -rf src/Consumer",
            "rm -rf src/Command",
            "rm -rf src/Migrations"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "~4.3"
        }
    }
}
